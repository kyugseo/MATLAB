      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "scatter(E,LHS(E),s=1)\n",
    "plot(E,RHSeven(E),'g')\n",
    "plot(E,RHSodd(E),'y')\n",
    "plot([0,5],[0,0],\"r--\")\n",
    "\n",
    "ylim(-5,5)\n",
    "legend(('LHS even','LHS odd'),loc=0)"
   ]
  },
  {
   "cell_type": "markdown",
