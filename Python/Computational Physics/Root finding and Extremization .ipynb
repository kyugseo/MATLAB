    }
   ],
   "source": [
    "b=0.07\n",
    "h=0\n",
    "Mstars=[]\n",
    "tlist=linspace(0.4,1.4,50)\n",
    "\n",
    "for t in tlist:\n",
    "    sol=opt.minimize(freeE,1,args=(t,b,h))\n",
    "    Mstars.append(sol.x)\n",
    "plot(tlist,Mstars,\".\")\n",
    "xlabel(\"$t$\")\n",
    "ylabel(\"$M$\")"
   ]
